@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix transit: <http://vocab.org/transit/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix vocab: <http://deportesMadrid#> .
@prefix apar: <http://deportesMadrid/ontologies/Aparcabicis#> .
@prefix pol: <http://deportesMadrid/ontologies/Insta_Deportiva#> .
@prefix ubi: <http://deportesMadrid/ubicacion#> .

<#TriplesMap3> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "./csv/instalaciones-deportivas-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ];
  rr:subjectMap [
    rr:class vocab:Ubicacion
  ];

  rr:predicateObjectMap [
    rr:predicate vocab:Barrio;
    rr:objectMap [
      rml:reference "BARRIO"
      ]
  ];

  rr:predicateObjectMap [
    rr:predicate vocab:Codigo-Postal;
    rr:objectMap [
      rml:reference "CODIGO-POSTAL"
      ]
  ];

    rr:predicateObjectMap [
    rr:predicate vocab:Distrito;
    rr:objectMap [
      rml:reference "DISTRITO"
      ]
  ].

<#TriplesMap1> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "./csv/instalaciones-deportivas-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ];
  rr:subjectMap [
    rr:template "http://deportesMadrid/instalaciones-deportivas/{PK}";
    rr:class vocab:Polideportivo
  ];

  rr:predicateObjectMap [
    rr:predicate vocab:Nombre;
    rr:objectMap [
      rml:reference "NOMBRE"
      ]
  ];

    rr:predicateObjectMap [
    rr:predicate vocab:PK;
    rr:objectMap [
      rml:reference "PK";
      rr:datatype <http://www.w3.org/2001/XMLSchema#integer>
    ]
  ];

    rr:predicateObjectMap [
    rr:predicate vocab:Tipo;
    rr:objectMap [
      rml:reference "TIPO"
      ]
  ];

    rr:predicateObjectMap [
    rr:predicate vocab:Equipamiento;
    rr:objectMap [
      rml:reference "EQUIPAMIENTO"
      ]
  ];

    rr:predicateObjectMap [
    rr:predicate vocab:Transporte;
    rr:objectMap [
      rml:reference "TRANSPORTE"
      ]
  ];

  rr:predicateObjectMap [
    rr:predicate vocab:URL;
    rr:objectMap [
      rml:reference "CONTENT-URL";
      ]
  ];

    rr:predicateObjectMap [
    rr:predicate vocab:Nombre-via;
    rr:objectMap [
      rml:reference "NOMBRE-VIA"
      ]
  ];

    rr:predicateObjectMap [
    rr:predicate vocab:Clase-vial;
    rr:objectMap [
      rml:reference "CLASE-VIAL"
      ]
    ].

  <#TriplesMap2> a rr:TriplesMap;
  rml:logicalSource [
    rml:source "./csv/aparcabicis-updated.csv" ;
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [
    rr:template "http://deportesMadrid/aparcabicis-updated/{ID}";
    rr:class vocab:Aparcabicis
  ];

  rr:predicateObjectMap [
    rr:predicate vocab:ID;
    rr:objectMap [
      rml:reference "ID";
      rr:datatype <http://www.w3.org/2001/XMLSchema#integer>
    ]
  ];
    rr:predicateObjectMap [
    rr:predicate vocab:Estado;
    rr:objectMap [
      rml:reference "ESTADO"
      ]
  ];
    rr:predicateObjectMap [
    rr:predicate vocab:Fecha-instalacion;
    rr:objectMap [
      rml:reference "FECHA-INSTALACION";
      rr:datatype xsd:dateTime
      ]
  ];
    rr:predicateObjectMap [
    rr:predicate vocab:Modelo;
    rr:objectMap [
      rml:reference "MODELO"
      ]
  ];
    rr:predicateObjectMap [
    rr:predicate vocab:Nombre-via;
    rr:objectMap [
      rml:reference "NOMBRE-VIA"
      ]
  ];
    rr:predicateObjectMap [
    rr:predicate vocab:Clase-vial;
    rr:objectMap [
      rml:reference "CLASE-VIAL"
      ]
  ];
    rr:predicateObjectMap [
    rr:predicate vocab:relation;
    rr:objectMap [
      rr:parentTriplesMap <#TriplesMap3>
    ]
  ].